{
  "name": "white-elephant-server",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "lint": "eslint ./src/**/*.ts",
    "lint:fix": "lint --fix",
    "docker:dev:up": "docker-compose -f dev-db.yml up -d",
    "docker:dev:down": "docker-compose -f dev-db.yml down",
    "docker:test:up": "docker-compose -f test-db.yml up -d",
    "docker:test:down": "docker-compose -f test-db.yml down",
    "pretest": "npm run docker:test:up && dotenv -e .env.test -- npx prisma migrate dev && echo 'pretest complete'",
    "test": "dotenv -e .env.test -- jest -i --watch || :",
    "posttest": "npm run docker:test:down",
    "start": "ts-node src/index.ts",
    "db:dev": "npm run docker:dev:up && dotenv -e .env.development -- npx prisma migrate dev",
    "prisma:migrate:dev": "dotenv -e .env.development -- npx prisma migrate dev --name",
    "prisma:studio": "npm run docker:dev:up && npx prisma studio",
    "nodemon": "dotenv -e .env.development nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "ts-node": "ts-node src/index.ts",
    "dev": "npm run db:dev && npm run nodemon",
    "gql-gen": "graphql-codegen --config codegen.yml",
    "ts": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@apollo/rover": "^0.4.1",
    "@graphql-codegen/cli": "2.3.0",
    "@graphql-codegen/introspection": "2.1.0",
    "@graphql-codegen/typescript": "2.4.1",
    "@graphql-codegen/typescript-resolvers": "2.4.2",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "prisma": "^3.6.0",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.3"
  },
  "dependencies": {
    "@graphql-tools/schema": "^8.3.1",
    "@prisma/client": "^3.6.0",
    "apollo-server": "^3.5.0",
    "apollo-server-express": "^3.5.0",
    "dotenv": "^10.0.0",
    "dotenv-cli": "^4.1.1",
    "graphql": "^16.1.0",
    "graphql-redis-subscriptions": "^2.4.2",
    "graphql-subscriptions": "^2.0.0",
    "lodash": "^4.17.21",
    "subscriptions-transport-ws": "^0.11.0"
  }
}
